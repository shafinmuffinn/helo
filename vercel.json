{
    "builds": [{
      "src": "helo/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.9" }
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static",
      "config": {
        "distDir": "static"
      }
    }
    ],
    "routes": [
        {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "helo/wsgi.py"
      }
    ]
}
